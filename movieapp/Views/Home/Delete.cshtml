@{
    ViewData["Title"] = "Delete Movie";
}
@model formResponse
<div class="text-center">
    <h2>Confirm Deletion</h2>
    <h3>Are you sure you want to delete the record for @Model.Title?</h3>

</div>


<div class="text-center">

    <form method="post" asp-action="Delete">
        <button type="submit">Delete</button>
        <a asp-controller="Home" asp-action="MovieList" class="button">Cancel</a>

        <input type="hidden" asp-for="MovieNumber" />
    </form>
</div>